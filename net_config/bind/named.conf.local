zone "esxlab.local" {
        type master;
        file "/etc/bind/zones/esxlab.local.db";
        };

# This is the zone definition for reverse DNS. replace 20.20.10 with your network address in reverse notation - e.g my network address is 10.20.20
zone "20.20.10.in-addr.arpa" {
     type master;
     file "/etc/bind/zones/20.20.10.in-addr.arpa";
};
